{
  "expo": {
    "name": "Music",
    "slug": "MusicApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/discicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/discicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Music",
      "newArchEnabled": true,
      "infoPlist": {
        "NSAppleMusicUsageDescription": "Allow Music to access your music library to play local songs.",
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/discicon.png",
        "backgroundColor": "#0f172a"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_MEDIA_PLAYBACK",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.WAKE_LOCK",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED"
      ],
      "package": "com.ram.musicapp",
      "newArchEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Music to access your photos.",
          "savePhotosPermission": "Allow Music to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "foregroundServiceType": "mediaPlayback"
          }
        }
      ],
      "react-native-track-player"
    ],
    "owner": "ram1997",
    "extra": {
      "eas": {
        "projectId": "585b831f-6a65-4e72-aa80-242256894552"
      }
    }
  }
}
